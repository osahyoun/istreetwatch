<div class='row section'>
  <div class='col s12'>

    <h2 class="center">New Report</h2>
    <%= render 'form', report: @report %>
    <% if params[:partners].present? %>
      <p class='center position-bottom'>Powered by <%= link_to 'iStreetWatch', root_url, target: '_blank' %></p>
    <% end %>

  </div>
</div>

<script>
  function init() {
    initAutocomplete();
  }
</script>

<%# Required for Google's location API %>
<% unless Rails.env.test? %>
  <script src="https://maps.googleapis.com/maps/api/js?key=<%= Rails.application.secrets.google_api_key %>&callback=init&libraries=places" async defer></script>
<% end %>